{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "noEmit": true,
    "strict": true,
    "paths": {
      "*": ["*"],
      "tests": ["tests/*"],
      "@components/*": ["components/*"]
    },
    "types": ["node"]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
  ],
  "exclude": ["node_modules"]
}
